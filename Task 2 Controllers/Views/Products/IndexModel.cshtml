@model List<Product>
@{
    Layout = null;
}


<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <div>
        @foreach (var item in Model)
        {
            <li>
                @item.Id
                @item.Name
                @item.Price
            </li>
        }
    </div>
</body>
</html>
